<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>userServlet</servlet-name>
    <servlet-class>User</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>userServlet</servlet-name>
    <url-pattern>/addUser</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>userLogin</servlet-name>
    <servlet-class>Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>userLogin</servlet-name>
    <url-pattern>/loginUser</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>deliveryBoyClass</servlet-name>
    <servlet-class>DeliveryBoy</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>deliveryBoyClass</servlet-name>
    <url-pattern>/deliveryBoy</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>productClass</servlet-name>
    <servlet-class>Product</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>productClass</servlet-name>
    <url-pattern>/products</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>cartServlet</servlet-name>
    <servlet-class>Cart</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>cartServlet</servlet-name>
    <url-pattern>/cart</url-pattern>
  </servlet-mapping>
</web-app>
